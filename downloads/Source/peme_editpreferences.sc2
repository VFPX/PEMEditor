	PROCEDURE Frame.PageHighlighting.Frame.pagProcessing1.cmdDefaultAssign.MouseEnter
		LPARAMETERS nButton, nShift, nXCoord, nYCoord
		
		This.ToolTipText = ccLOC_Pref_Tip_Default_Assign
		
	ENDPROC

	PROCEDURE Frame.PageHighlighting.Frame.pagProcessing1.cmdDefaultAssign.RightClick
		Local loMenu As 'ContextMenu' Of  'PEME_Menu.vcx'
		Local lcChoice, lcCode, lcNewCode
		
		loMenu = Newobject ('ContextMenu', 'PEME_Menu.vcx')
		
		With loMenu
			If .AddSubMenu('Simple (non-array) properties')
				.AddMenuItem(pf_LOC_CMA_Edit, 'Edit Scalar')
				.AddMenuItem(pf_LOC_CMA_Reset, 'Scalar Default')
				.EndSubmenu()
			Endif
		
			If .AddSubMenu('Array properties')
				.AddMenuItem(pf_LOC_CMA_Edit, 'Edit Array')
				.AddMenuItem(pf_LOC_CMA_Reset, 'Array Default')
				.EndSubmenu()
			Endif
		Endwith
		
		If Not loMenu.Activate()
			Return
		Endif
		
		lcChoice = loMenu.Keyword
		
		Do Case
		
			Case lcChoice = 'Edit Scalar'
				lcCode = Thisform.oPrefs.cAssignCode
				lcNewCode = Thisform.EditDefaultAssign(ccLOC_Pref_ID_Default_Assign, lcCode)
				Thisform.oPrefs.cAssignCode = lcNewCode
		
			Case lcChoice = 'Scalar Default'
				Thisform.oPrefs.cAssignCode = .Null.
		
			Case lcChoice = 'Edit Array'
				lcCode = Thisform.oPrefs.cArrayAssignCode
				lcNewCode = Thisform.EditDefaultAssign(ccLOC_Pref_ID_Default_Assign, lcCode)
				Thisform.oPrefs.cArrayAssignCode = lcNewCode
		
			Case lcChoice = 'Array Default'
				Thisform.oPrefs.cArrayAssignCode = .Null.
		
		Endcase
		
		
	ENDPROC

