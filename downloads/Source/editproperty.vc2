	PROCEDURE savesettings
		Local lcColumnWidths, lcColumnOrder, loColumn, lnI, lbPutFlag
		
		With This
		
			.savedockinginfo()
		
			.nSplitterTop		= .oSplitterV.Top
			.nSplitterLeft		= .oSplitterH.Left
			.nDescSplitterTop	= .cntControls.oSplitter.Top
		
			.GetPutSettings(.T.)
			.GetMRUSettings(.T.)
		
