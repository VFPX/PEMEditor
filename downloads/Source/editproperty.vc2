	PROCEDURE cntGridControls.GotFocus
		DoDefault()
		
		If ('L' = Type("Thisform.oMember.IsNewMember")) 			;
				and Thisform.oMember.IsNewMember					;
				and Between (Recno ("csrMembers"), 1, Reccount("csrMembers")) 
