		lparameters tcStr
		local luRetVal
		do case
			case inlist(upper(alltrim(tcStr)), [.T.], [.F.])
			luRetVal = tcStr = [.T.]
			case chrtran(tcStr, chrtran(tcStr, [0123456789], []), []) = tcStr
				luRetVal = int(val(tcStr))
			otherwise
				luRetVal = tcStr
		endcase
		return luRetVal
		
	ENDPROC

