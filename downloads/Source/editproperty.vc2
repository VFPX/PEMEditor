	PROCEDURE cntDescription.edtDescription.KeyPress
		* Don't allow a CR.
		
		lparameters tnKeyCode, ;
			tnShiftAltCtrl
		if tnKeyCode = 13 and not Thisform.lAllowDescriptionCRs 
			Declare Integer MessageBeep In Win32API Integer wType
			MessageBeep(MB_ICONSTOP)
			nodefault
		else
