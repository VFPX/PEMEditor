				Endwith
			Endif
		
			DoDefault()
			Goto Top
		
			.Refresh()
		
		Endwith
		
		
		
	ENDPROC

	PROCEDURE Load
		DoDefault()
		Local lcAlias, lnRow
		
		With This
			.oServer = _oPEMEditor.oUtils.oServer
			.CreateCompareCursor()
		Endwith
		
		
	ENDPROC

	PROCEDURE loadsettings
		This.cMembersSortOrder = .cPasteOrder
		This.LoadColumnInfo(This.gridCompare)
		DoDefault()
		
		
		
	ENDPROC

	PROCEDURE savesettings
		.cPasteOrder = This.cMembersSortOrder
		This.SaveColumnInfo(This.gridCompare)
		
		DoDefault()
		
	ENDPROC

	PROCEDURE setdefaults
		.cPasteOrder = ''
	ENDPROC

	PROCEDURE sortgrid
		Lparameters lcMainSortField, lcAltSortField
		
	ENDPROC

	PROCEDURE Unload
		Return this.lPasteIt
	ENDPROC

ENDDEFINE

