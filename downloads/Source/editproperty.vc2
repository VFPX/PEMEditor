				lReadOnlyX 	L			, 	;  && ccReadOnlyXCol
				cDescript 	C (254)		, 	;  && ccDescriptCol
				nObjNumber	N (4)		,  	;  && ccObjNumberCol
				cValue		C (100)		,   ;  && ccPastecValueCol
