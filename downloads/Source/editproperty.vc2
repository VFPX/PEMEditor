	PROCEDURE setcurrentobjectproperty
		Lparameters loTarget, lcMethodName
		
		Local laMethods[1], lcSuffix
		
		This.SetCurrentSelectedObject (loTarget)
		Select csrMEMBERS
		Locate For Upper (cName) = Upper (lcMethodName + ' ')
		If Not Found()
