	ENDPROC

	PROCEDURE getvalue
		Lparameters lcType, lxValue, llHasCode
		
		Do Case
			Case lcType $ 'ME' And Not Empty (lxValue)
				Return '[User Procedure]'
			Case lcType $ 'ME' And llHasCode
				Return '[Inherited]'
			Case lcType $ 'ME'
				Return ''
			Case lcType = 'X'
				Return '=' + lxValue
			Case 'C' = Vartype (lxValue) And Empty (lxValue)
				Return '(None)'
