	PROCEDURE Init
		Lparameters lcVersion
		Local lnPos
		
		DoDefault()
		This.cVersion = lcVersion
		lnPos		  = At ('-', lcVersion)
		
		If lnPos # 0
			This.Caption = Alltrim (Left (lcVersion, lnPos - 1))
		Endif
	ENDPROC

